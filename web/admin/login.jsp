<form action="j_security_check" method="post">
    <div id="adminLoginBox">
        <p><strong><label for="username">username:</label>
                <input type="text" name="j_username" class="loginInput" id="username" autofocus></strong></p>

        <p><strong><label for="password">password:</label>
                <input type="password" name="j_password" class="loginInput" id="password"></strong></p>

        <p><input type="submit" name="login" id="button" value="Login">&nbsp;
            <input type="reset" name="reset" id="reset"></p>
    </div>
</form>
